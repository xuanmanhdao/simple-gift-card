<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">My Gift Card</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Mageplaza\SimpleGiftCard\Block\GiftCardHistory\Registry\ListGiftCard" name="giftcard_history"
                   template="Mageplaza_SimpleGiftCard::mygiftcard.phtml"
                   cacheable="false">
            </block>
        </referenceContainer>
    </body>
</page>
